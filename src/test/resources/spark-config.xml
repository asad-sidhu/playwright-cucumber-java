<extentreports>
    <configuration>
        <!--  report theme  -->
        <!--  standard, dark  -->
        <theme>dark</theme>
        <!--  document encoding  -->
        <!--  defaults to UTF-8  -->
        <encoding>UTF-8</encoding>
        <!--  protocol for script and stylesheets  -->
        <!--  defaults to https  -->
        <protocol>https</protocol>
        <!--  title of the document  -->
        <documentTitle>HN Cucumber Reports</documentTitle>
        <!--  report name - displayed at top-nav  -->
        <reportName>Automation Test Results</reportName>
        <!--  view report without internet  -->
        <offlineMode>false</offlineMode>
        <thumbnailForBase64>true</thumbnailForBase64>
        <!--  custom javascript  -->
        <js/>
        <!--  custom styles  -->
        <css/>
    </configuration>
</extentreports>